<div class="container">
  <div class="row">
    <div class="collective-name"><h1>Dame Jeanne</h1></div>
    <br>
    <ul>
      <% @artists_alpha.each do |artist| %>
        <div class="list-group d-none d-sm-block">
          <div id="artists-name">
            <%= link_to artist_path(artist) do %>
              <h5><%= "#{artist.first_name} #{artist.last_name}" %></h5>
            <% end %>
          </div>
        </div>
      <% end %>
    </ul>
  </div>

    <div class="cards-container">
      <div class="row">
        <div class="d-flex justify-content-around mr-auto p-1">
          <div class="card card-poster" style="background-image: url('https://res.cloudinary.com/sybillecrd/image/upload/v1540279457/Dame%20Jeanne/bordeaux_1g_petit.jpg');">
            <div class="card-filter">
              <div class="card-infos-artist">
                <h3>Marie Paruit</h3>
                <p>Illustratrice</p>
              </div>
            </div>
          </div>
        </div>

        <div class="carousel slide" data-ride="carousel">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img class="d-block w-100" src="https://res.cloudinary.com/sybillecrd/image/upload/v1540279457/Dame%20Jeanne/bordeaux_1g_petit.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="https://res.cloudinary.com/sybillecrd/image/upload/v1540279457/Dame%20Jeanne/bordeaux_1d_petit.jpg" alt="Second slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100" src="https://res.cloudinary.com/sybillecrd/image/upload/v1540279456/Dame%20Jeanne/pin-up-francaise-belle-vie-petit.jpg" alt="Third slide">
            </div>
            <div class="carousel-content list-group">
                <div class="carousel-content-name"><p>Marie Paruit</p></div>
                <div class="carousel-content-job"><p>Illustratrice</p></div>
            </div>
          </div>
        </div>

        <div class="d-flex justify-content-around mr-auto p-1">
          <div class="card card-poster" style="background-image: url('https://res.cloudinary.com/sybillecrd/image/upload/v1540279457/Dame%20Jeanne/bordeaux_1d_petit.jpg');">
            <div class="card-filter">
              <div class="card-infos-artist">
                <h3>Marie Paruit</h3>
                <p>Illustratrice</p>
              </div>
            </div>
          </div>
        </div>



      </div>
    </div>


</div>
